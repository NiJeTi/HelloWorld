{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "fileSizeLimitBytes": "1073741824"
        }
      }
    ]
  },
  "NameHolder": {
    "DefaultName": "Tester"
  }
}
